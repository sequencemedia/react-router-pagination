{
  "compilerOptions": {
    "jsx": "react",
    "module": "NodeNext",
    "target": "ESNext",
    "moduleResolution": "NodeNext",
    "noEmit": true,
    "strict": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "baseUrl": ".",
    "paths": {
      "#pagination": [
        "src/pagination/index.tsx"
      ],
      "#pagination/pagination/component": [
        "src/pagination/component.tsx"
      ],
      "#pagination/super/pagination/component": [
        "src/super/pagination/component.tsx"
      ],
      "#pagination/*": [
        "src/pagination/*.tsx"
      ]
    }
  },
  "include": [
    "."
  ]
}
